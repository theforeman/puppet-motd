{
  "name": "theforeman-motd",
  "version": "2.1.0",
  "author": "theforeman",
  "summary": "Foreman infra motd",
  "license": "GPL-3.0+",
  "source": "https://github.com/theforeman/puppet-motd",
  "project_page": "https://github.com/theforeman/puppet-motd",
  "issues_url": "https://github.com/theforeman/puppet-motd/issues",
  "description": "Module for configuring motd on the Foreman infrastructure. Also used in CI pipelines",
  "tags": [
    "foreman",
    "motd"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.25.0 < 10.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.0.0 < 9.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04"
      ]
    },
    {
      "operatingsystem": "AlmaLinux",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    }
  ]
}
